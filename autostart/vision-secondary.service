# Vision Secondary Service
[Unit]
Description=FRC 1073 Vision Secondary Service
After=network.target

[Service]
Type=simple
Restart=always
RestartSec=1
User=team1073
WorkingDirectory=/home/team1073/vision20/scripts
ExecStart=/home/team1073/vision20/scripts/webcam /dev/video1 10.10.73.200

[Install]
WantedBy=multi-user.target
