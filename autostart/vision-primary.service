# Vision Primary Service
[Unit]
Description=FRC 1073 Vision Primary Service
After=network.target

[Service]
Type=simple
Restart=always
RestartSec=1
User=todd
WorkingDirectory=/home/todd/Projects/nanovision/scripts
ExecStart=/home/todd/Projects/nanovision/scripts/nano 127.0.0.1

[Install]
WantedBy=multi-user.target
